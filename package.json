{
  "name": "react-sample-project",
  "version": "0.0.1",
  "description": "a sample project for react and browserify users",
  "repository": {
    "type": "git",
    "url": "https://github.com/FEWorkflow/react-sample-project"
  },
  "keywords": [
    "gulp",
    "react",
    "browserify",
    "babel",
    "react-starter"
  ],
  "scripts": {
    "publish": "node node_modules/gulp/bin/gulp.js",
    "build-app": "node node_modules/gulp/bin/gulp.js build-app",
    "build-common": "node node_modules/gulp/bin/gulp.js build-common",
    "watch": "node node_modules/gulp/bin/gulp.js watch",
    "test": "jest"
  },
  "author": "simonxu",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.0.2",
    "history": "1.13.1",
    "lodash": "^3.10.1",
    "rc-pagination": "^1.2.1",
    "rc-table": "^3.6.1",
    "react": "^0.14.2",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.2",
    "react-highcharts": "^5.0.4",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "superagent": "^1.4.0",
    "superagent-mocker": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "del": "^2.1.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.8.0",
    "gulp": "^3.9.0",
    "gulp-concat-css": "^2.2.0",
    "gulp-hasher": "^0.1.0",
    "gulp-html-replace": "^1.5.4",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "jest-cli": "^0.8.0",
    "react-addons-test-utils": "^0.14.3",
    "vinyl-source-stream": "^1.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
